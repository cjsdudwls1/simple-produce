"        for transaction in self.transactions:\n","            summary += f\"- {transaction}\\n\"\n","        return summary\n","\n","\n","# 계좌 생성\n","account_dad = BankAccount(\"천영진 아빠\")\n","account_son = BankAccount(\"천영진\")\n","\n","# 누적된 output을 저장할 변수\n","global_output = \"\"\n","\n","# 함수 정의 (입금/출금 처리 및 결과 출력)\n","def bank_transaction(name, action, amount, reason, date=None):\n","    global global_output  # 누적된 output을 참조\n","\n","    amount = int(amount)\n","\n","    # 계좌 선택\n","    if name == \"천영진 아빠\":\n","        account = account_dad\n","        other_account = account_son\n","    elif name == \"천영진\":\n","        account = account_son\n","        other_account = account_dad\n","    else:\n","        return \"잘못된 계좌 이름입니다. '천영진 아빠' 또는 '천영진' 중 선택하세요.\"\n","\n","    # 입금 처리\n","    if action == \"입금\":\n","        account.deposit(amount, date)\n","\n","    # 출금 처리\n","    elif action == \"출금\":\n","        if reason == \"송금 (상대방 계좌로)\":\n","            # 상대방 계좌에 입금 처리\n","            if account.balance >= amount:\n","                account.withdraw(amount, date)\n","                other_account.deposit(amount, date)\n","                global_output += f\"{name}가 {amount}원을 송금했습니다.\\n\"\n","            else:\n","                return f\"잔액이 부족합니다. 현재 잔액: {account.balance}원\"\n","        else:  # 기타 지출\n","            result = account.withdraw(amount, date)\n","            if result:\n","                return result\n","\n","    # 거래 내역 누적\n","    account_summary = account.show_account_summary()\n","    other_account_summary = other_account.show_account_summary() if reason == \"송금 (상대방 계좌로)\" else \"\"\n","    global_output += f\"{account_summary}\\n\"  # 누적 출력\n","    if other_account_summary:\n","        global_output += f\"{other_account_summary}\\n\"  # 상대방 계좌의 변화도 출력\n","    return global_output\n","\n","# Action에 따라 출금 사유 선택 활성화 여부 변경\n","def update_reason_input(action):\n","    if action == \"출금\":\n","        return gr.update(visible=True)\n","    else:\n","        return gr.update(visible=False)\n","\n","\n","# Gradio 인터페이스 정의\n","with gr.Blocks() as demo:\n","    name_input = gr.Dropdown([\"천영진 아빠\", \"천영진\"], label=\"Name\")  # 드롭다운으로 선택\n","    action_input = gr.Radio([\"입금\", \"출금\"], label=\"Action\")\n","    amount_input = gr.Number(label=\"Amount\")\n","    date_input = gr.Textbox(label=\"Date (YYYY-MM-DD)\", placeholder=\"날짜를 입력하거나 비워두세요\")\n","\n","    # 출금 사유 선택 (송금 또는 기타 지출)\n","    reason_input = gr.Dropdown([\"송금 (상대방 계좌로)\", \"기타 지출\"], label=\"Reason for Withdrawal\", visible=False)\n","\n","    output = gr.Textbox(label=\"Output\")\n","\n","    submit_btn = gr.Button(\"Submit\")\n","\n","    # Action에 따라 출금 사유 보이기/숨기기 처리\n","    action_input.change(fn=update_reason_input, inputs=action_input, outputs=reason_input)\n","\n","    submit_btn.click(fn=bank_transaction,\n","                     inputs=[name_input, action_input, amount_input, reason_input, date_input],\n","                     outputs=output)\n","\n","# Gradio 앱 실행\n","demo.launch()\n"],"metadata":{"id":"558ucmUfWbro","executionInfo":{"status":"ok","timestamp":1726818783186,"user_tz":-540,"elapsed":1392,"user":{"displayName":"","userId":""}},"outputId":"162cd476-86ae-48f8-e746-867820e7e751","colab":{"base_uri":"https://localhost:8080/","height":645}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Setting queue=True in a Colab notebook requires sharing enabled. Setting `share=True` (you can turn this off by setting `share=False` in `launch()` explicitly).\n","\n","Colab notebook detected. To show errors in colab notebook, set debug=True in launch()\n","Running on public URL: https://65968423a7f471f92b.gradio.live\n","\n","This share link expires in 72 hours. For free permanent hosting and GPU upgrades, run `gradio deploy` from Terminal to deploy to Spaces (https://huggingface.co/spaces)\n"]},{"output_type":"display_data","data":{"text/plain":["<IPython.core.display.HTML object>"],"text/html":["